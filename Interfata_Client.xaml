                <ImageBrush ImageSource="shopping-cart.png"/>
            </Ellipse.Fill>
        </Ellipse>
        <Label x:Name="Title" Content="" HorizontalAlignment="Left" Margin="213,24,0,0" VerticalAlignment="Top" Width="410" Height="66"/>
