            <Button.Background>
                <ImageBrush ImageSource="operator.png"/>
            </Button.Background>
        </Button>
        <Grid x:Name="GMeniu" HorizontalAlignment="Left" Height="481" Margin="112,3,-0.4,0" VerticalAlignment="Top" Width="682" Grid.RowSpan="2"/>
